{
  "repository": {
    "name": "europeana",
    "host_name": "europeana",
    "host_netloc": "https://www.europeana.eu",
    "api": {
        "base": "https://api.europeana.eu/oai/record?",
        "params": {
          "verb": "GetRecord",
          "metadataPrefix": "edm",
          "identifier": "/$collection/$record_id"
        }
    },
    "url": {"regex": "^.*\/([\\w]+)(\/|\\.)(?P<record_id>[\\w\\W]+)$", "format": "$api"},
    "hdl_id": "11304",
    "doi_id": "10.23728",
    "parser": {
      "type": "xml",
      "config":{
        "nsmap": {
          "edm": "http://www.europeana.eu/schemas/edm/",
          "dc": "http://purl.org/dc/elements/1.1/",
          "ore": "http://www.openarchives.org/ore/terms/"
        },
        "license": ".//edm:rights",
        "description": ".//dc:description",
        "ref_file": {
          "path": ".//edm:WebResource[@rdf:about]"
        }
      }
    },
    "test_examples": {
      "url": "",
      "hdl": ""}
  }
}
